<div class="form">
  <%= form_for @movie do |f| %>
  <div class="context">
    <%= f.label :mylist_id %>
    <%= collection_select :movie, :mylist_id, User.find(current_user).mylists, :id, :name %>
  </div>
  <div class="context">
    <%= f.label :sm_id %>
    <%= f.text_field :sm_id %>
  </div>
  <div class="context">
    <%= f.label :thread_id %>
    <%= f.text_field :thread_id %>
  </div>
  <div class="submit">
    <%= f.submit %>
  </div>
  <% end %>
</div>
